		x:Class="Learning.Navigation.MenuPage">
	<ContentPage.Content>
		<StackLayout>
			<ListView x:Name="lstStudents" ItemsSource="{Binding Students}"  >
				<ListView.ItemTemplate> 
					<DataTemplate>
					 <ImageCell Text="{Binding Name}"></ImageCell>
					<!--<Entry Text="{Binding Name}"></Entry>-->
						
						
					</DataTemplate>

				</ListView.ItemTemplate>
			</ListView>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>